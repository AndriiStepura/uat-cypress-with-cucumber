{
  "name": "uat-cypress-with-cucumber",
  "version": "1.0.0",
  "description": "Boilerplate for UAT tests with Cypress and Cucumber",
  "main": "index.js",
  "devDependencies": {
    "cypress": "^13.6.0",
    "cypress-cucumber-preprocessor": "^4.3.1"
  },
  "cypress-cucumber-preprocessor": {
    "nonGlobalStepDefinitions": false,
    "step_definitions": "cypress/e2e/cucumber/Tests"
  },  
  "scripts": {
    "test": "cypress run --browser=chrome --headless",
    "test-see": "cypress run --browser=chrome --headed",
    "tests-bdd": "yarn run cypress open"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/AndriiStepura/uat-cypress-with-cucumber.git"
  },
  "keywords": [
    "uat","cypress","cucumber"
  ],
  "author": "Andrii Stepura",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/AndriiStepura/uat-cypress-with-cucumber/issues"
  },
  "homepage": "https://github.com/AndriiStepura/uat-cypress-with-cucumber#readme"
}
